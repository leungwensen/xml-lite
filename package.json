    "gulp-shell": "~0.5.2",
    "gulp-util": "~3.0.7",
    "jquery": "~3.1.0",
    "json-loader": "~0.5.4",
    "lebab": "~2.3.0",
    "less": "~2.7.1",
    "less-loader": "~2.2.3",
    "lesshat": "~4.1.0",
    "mocha": "~3.0.2",
    "normalize.css": "~4.2.0",
    "optimize-css-assets-webpack-plugin": "~1.3.0",
    "pre-commit": "~1.1.3",
