{
  "name": "xml-lite",
  "version": "0.7.9",
  "description": "maintaining xml in pure javascript (IN BOTH NODE.JS & BROWSERS) ",
  "author": "leungwensen",
  "homepage": "https://github.com/leungwensen/xml-lite#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "keywords": [
    "xml",
    "xml2js",
    "js2xml",
    "dom",
    "xml-lite"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leungwensen/xml-lite.git"
  },
  "bugs": {
    "url": "https://github.com/leungwensen/xml-lite/issues"
  },
  "bin": {
    "xml-lite": "./bin/xml-lite.js"
  },
  "scripts": {
    "build-doc": "zfinder build",
    "eslint": "gulp eslint",
