    "babel-core": "~6.14.0",
    "babel-loader": "~6.2.5",
    "babel-preset-es2015": "~6.14.0",
    "chai": "~3.5.0",
    "css-loader": "~0.24.0",
    "cssnano": "~3.7.4",
    "eslint": "~3.3.1",
    "eslint-config-airbnb": "~10.0.1",
    "eslint-plugin-import": "~1.14.0",
    "eslint-plugin-jsx-a11y": "~2.1.0",
    "eslint-plugin-react": "~6.1.2",
    "extract-text-webpack-plugin": "~1.0.1",
    "file-loader": "~0.9.0",
    "gulp": "~3.9.1",
    "gulp-babel": "~6.1.2",
    "gulp-eslint": "~3.0.1",
